<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Catalog::catalog">
                    <resource id="Magento_Catalog::catalog_inventory">
                        <resource id="Aislan_MovieCatalog::catalog_movies" title="Movies" translate="title" sortOrder="30"/>
                        <resource id="Aislan_MovieCatalog::movies_save" title="Movies Save" translate="title" sortOrder="40"/>
                        <resource id="Aislan_MovieCatalog::movies_delete" title="Movies Delete" translate="title" sortOrder="50"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
